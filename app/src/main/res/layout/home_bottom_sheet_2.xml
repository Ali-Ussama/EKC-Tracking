<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/bottom_sheet"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:background="@color/transparent"
    android:orientation="vertical"
    app:behavior_hideable="true"
    app:layout_behavior="com.ekc.ekctracking.view.activities.mainActivity.LockableBottomSheetBehavior"
    tools:context=".view.activities.mainActivity.MainActivity">
    <!--    app:layout_behavior="com.ekc.ekctracking.view.activities.mainActivity.LockableBottomSheetBehavior"-->
    <!--    app:layout_behavior="@string/bottom_sheet_behavior"-->

    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content">

        <ViewAnimator
            android:id="@+id/home_bottom_sheet_car_details_view_animator"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent">


            <androidx.cardview.widget.CardView
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginStart="@dimen/_1sdp"
                android:layout_marginEnd="@dimen/_1sdp"
                app:cardCornerRadius="@dimen/_12sdp">

                <androidx.constraintlayout.widget.ConstraintLayout
                    android:id="@+id/home_bottom_sheet_car_details_container"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:visibility="visible">

                    <View
                        android:id="@+id/home_bottom_sheet_car_details_toolbar"
                        android:layout_width="0dp"
                        android:layout_height="?actionBarSize"
                        android:background="@color/colorPrimary"
                        app:layout_constraintEnd_toEndOf="parent"
                        app:layout_constraintStart_toStartOf="parent"
                        app:layout_constraintTop_toTopOf="parent" />

                    <ImageView
                        android:id="@+id/home_bottom_sheet_car_details_close_ic"
                        android:layout_width="@dimen/_24sdp"
                        android:layout_height="0dp"
                        app:layout_constraintBottom_toBottomOf="@id/home_bottom_sheet_car_details_toolbar"
                        app:layout_constraintStart_toStartOf="@+id/home_bottom_sheet_car_details_toolbar"
                        app:layout_constraintTop_toTopOf="parent"
                        app:srcCompat="@drawable/ic_close_white_24dp" />

                    <ImageView
                        android:id="@+id/home_bottom_sheet_car_details_expand_collapse_ic"
                        android:layout_width="@dimen/_24sdp"
                        android:layout_height="0dp"
                        app:layout_constraintBottom_toBottomOf="@id/home_bottom_sheet_car_details_toolbar"
                        app:layout_constraintEnd_toEndOf="@+id/home_bottom_sheet_car_details_toolbar"
                        app:layout_constraintTop_toTopOf="@id/home_bottom_sheet_car_details_toolbar"
                        app:srcCompat="@drawable/ic_keyboard_arrow_down_white_24dp" />

                    <TextView
                        android:id="@+id/home_bottom_sheet_car_details_title_tv"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="بيانات السيارة"
                        android:textColor="@color/white"
                        android:textSize="@dimen/_14ssp"
                        app:layout_constraintBottom_toBottomOf="@id/home_bottom_sheet_car_details_toolbar"
                        app:layout_constraintEnd_toEndOf="@id/home_bottom_sheet_car_details_toolbar"
                        app:layout_constraintStart_toStartOf="@+id/home_bottom_sheet_car_details_toolbar"
                        app:layout_constraintTop_toTopOf="parent" />


                    <androidx.constraintlayout.widget.ConstraintLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        app:layout_constraintEnd_toEndOf="parent"
                        app:layout_constraintTop_toBottomOf="@+id/home_bottom_sheet_car_details_toolbar">

                        <androidx.constraintlayout.widget.ConstraintLayout
                            android:id="@+id/home_bottom_sheet_car_details_car_no_container"
