                            app:layout_constraintTop_toTopOf="parent"
                            app:srcCompat="@drawable/ic_directions_car_grey_24dp">

                            <ImageView
                                android:id="@+id/home_bottom_sheet_car_details_car_no_ic"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_marginTop="@dimen/_4sdp"
                                app:layout_constraintEnd_toEndOf="parent"
                                app:layout_constraintTop_toTopOf="parent"
                                app:srcCompat="@drawable/ic_directions_car_grey_24dp" />

                            <TextView
                                android:id="@+id/home_bottom_sheet_car_details_car_no_value"
                                android:layout_width="0dp"
                                android:layout_height="wrap_content"
                                android:layout_marginEnd="@dimen/_12sdp"
                                android:text="ب ط ر 5 4 2 3"
                                android:textColor="@color/dark_blue"
                                android:textSize="@dimen/_10ssp"
                                app:layout_constraintBottom_toBottomOf="@id/home_bottom_sheet_car_details_car_no_ic"
                                app:layout_constraintEnd_toStartOf="@id/home_bottom_sheet_car_details_car_no_ic"
                                app:layout_constraintTop_toTopOf="@id/home_bottom_sheet_car_details_car_no_ic" />


